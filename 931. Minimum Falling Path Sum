   int ans = Integer.MAX_VALUE;
        int [][] dp = new int[matrix.length][matrix[0].length];
        for(int i = 0; i < matrix[0].length; i++){
             dp[dp.length - 1][i] = matrix[dp.length - 1][i];
        }
        for(int row = dp.length - 2; row >= 0; row --){
            for(int col = 0; col < dp[0].length; col ++){
                 ans = Integer.MAX_VALUE;
                 if(col - 1 >= 0){
                 ans = Math.min(ans, matrix[row][col] + dp[row + 1][col - 1]);
                     }
                 ans = Math.min(ans, matrix[row][col] +  dp[row + 1][col]);
                if(col + 1 <= matrix[0].length - 1){
              ans = Math.min(ans, matrix[row][col] + dp[row + 1][col + 1]);
               }
                dp[row][col] = ans;
            }
        } ans = Integer.MAX_VALUE;
        for(int i = 0; i < matrix[0].length; i++){
           ans = Math.min(dp[0][i], ans);
        }
        return ans;
